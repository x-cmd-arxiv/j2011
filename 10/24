ificant macroeconomic\nassociation.\n","versions":[{"version":"v1","created":"Mon, 24 Oct 2011 15:09:58 GMT"},{"version":"v2","created":"Thu, 28 Nov 2013 09:10:54 GMT"}],"update_date":"2013-12-02"}
